<h1> HELLO</h1>
<div class='row'>
  <div class='col-md-3'>
    <table class='table table-bordered table-hover table-striped table-two-pane'>
      <thead>
        <th>Name</th>
      </thead>
      <tbody>
      {{#each }}
      <tr>
        <td>
          {{#if isSaving}}
          {{fa-icon 'fa-refresh' spin=true}}
          {{/if}}
          {{#link-to 'puppetclasses.show' this}}
          {{name}}
          {{/link-to}}
        </td>
      </tr>
      {{/each}}
      </tbody>
    </table>
  </div>
  {{outlet}}
</div>
